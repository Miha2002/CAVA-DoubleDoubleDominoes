    "        line2 = str(piece2[0])+str(piece2[2])+\" \"+str(second_number)+\"\\n\"\n",
    "        line3 = str(current_score)\n",
    "        # Afisare in consola\n",
    "        print(\"Joc \",game_number,\" mutarea \",round_number,\":\\n\",line1,line2,line3,\"\\nScore tab:\",score_tab,\"\\n\\n\")\n",
    "        \n",
    "        # Afisare in .txt\n",
    "        with open(folder_output + file[:-4]+'.txt', 'w') as f:\n",
    "            f.write(line1)\n",
    "            f.write(line2)\n",
    "            f.write(line3)\n",
    "    \n",
