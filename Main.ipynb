   "source": [
    "files = os.listdir(folder)\n",
    "verif = np.zeros(100)\n",
    "for file in files:\n",
    "    if file[-3:]=='jpg':\n",
    "        first = False\n",
    "        img = cv.imread(folder + file)\n",
    "        \n",
    "        split_result = file.split('_')\n",
    "        game_number = int(split_result[0])\n",
    "        round_number = int(split_result[1][:2])\n",
    "        with open(folder + str(game_number) + '_mutari.txt') as moves_file:\n",
    "            moves = moves_file.readlines()  \n",
    "        current_player = int(moves[round_number-1][-2])\n",
    "        \n",
    "        if verif[game_number] == 0: # prima mutare dintr-un joc\n",
    "            # maxim 6 playeri intr-un joc oficial\n",
    "            # trebuie initializat de fiecare daca cand incepe un joc nou\n",
    "            score_tab = np.zeros(6)\n",
    "            verif[game_number] = game_number\n",
    "            _, top_left,top_right,bottom_left,bottom_right = extrage_careu(img)\n",
    "            square_vertical = round((bottom_right[1]-top_left[1])/15)\n",
    "            square_horizontal = round((bottom_right[0]-top_left[0])/15)\n",
    "            \n",
    "        cropped_current_img = img[int(top_left[1]):int(bottom_right[1]+20),\n",
    "                            int(top_left[0]):int(bottom_right[0])+20]\n",
    "        # show_image('a',cropped_current_img)\n",
    "        measure = cropped_current_img.shape\n",
    "        \n",
    "        if round_number == 1:\n",
    "            # Daca aceasta este prima mutare din joc, ne trebuie imaginea cu jocul fara piese\n",
    "            prev_img = cv.imread(empty_board)\n",
    "            first = True\n",
    "            _, top_left_prev,top_right_prev,bottom_left_prev,bottom_right_prev = extrage_careu(prev_img)\n",
    "            cropped_prev_img = prev_img[int(top_left_prev[1]):int(bottom_right_prev[1])+20,\n",
    "                            int(top_left_prev[0]):int(bottom_right_prev[0])+20]\n",
    "        else:\n",
    "            cropped_prev_img = prev_img[int(top_left[1]):int(bottom_right[1])+20,\n",
    "                            int(top_left[0]):int(bottom_right[0])+20]\n",
    "        \n",
    "        if round_number == 1:\n",
    "            cropped_prev_img = cv.resize(cropped_prev_img,(measure[1],measure[0]))\n",
    "            \n",
    "        difference_result = image_difference(cropped_prev_img,cropped_current_img)\n",
    "        # show_image('diff',difference)\n",
    "        \n",
    "        piece1, piece2, isHorizontal = piece_position(difference_result)\n",
    "        coord_top_left = [(piece1[1]-1) * square_horizontal,\n",
    "                          (piece1[0]-1) * square_vertical]\n",
    "        if isHorizontal:\n",
    "            coord_bottom_right = [coord_top_left[0] + 2 * square_horizontal,\n",
    "                                  coord_top_left[1] + square_vertical]\n",
    "        else:\n",
    "            coord_bottom_right = [coord_top_left[0] + square_horizontal,\n",
    "                                  coord_top_left[1] + 2 * square_vertical]\n",
    "        \n",
    "        temp_img = cropped_current_img.copy()\n",
    "        temp_img = cv.copyMakeBorder(temp_img, 10,10,10,10, borderType=cv.BORDER_CONSTANT,dst=None,value=[0,0,0])\n",
    "    \n",
    "        piece_image = cropped_current_img[coord_top_left[1]:coord_bottom_right[1] + 20,\n",
    "                    coord_top_left[0]:coord_bottom_right[0] + 20]\n",
    "        height, width, _ = piece_image.shape\n",
    "        if isHorizontal:\n",
    "            width_cutoff = width // 2\n",
    "            square1 = piece_image[:, :width_cutoff+10]\n",
    "            square2 = piece_image[:, width_cutoff-10:]\n",
    "        else:\n",
    "            height_cutoff = height // 2\n",
    "            square1 = piece_image[:height_cutoff+10, :]\n",
    "            square2 = piece_image[height_cutoff-10:, :]\n",
    "            \n",
    "        # show_image('piece_image', piece_image)\n",
    "        # show_image('square 1', square1)\n",
    "        # show_image('square 2', square2)\n",
    "        \n",
    "        # Obtinem numerele piesei\n",
    "        first_number = number_classification(square1)\n",
    "        second_number = number_classification(square2)\n",
    "        \n",
    "        # Poza curenta devine poza anterioara pentru urmatoarea mutare        \n",
    "        prev_img = img\n",
    "        \n",
    "        # Calculam scorul\n",
    "        # punctaj = pozitie pion => [x] = nr piesa de sub pion \n",
    "        score1 = main_game[piece1[0]][piece1[1]]\n",
    "        score2 = main_game[piece2[0]][piece2[1]]\n",
    "        current_score = 0\n",
    "        \n",
    "        # Daca un numar de pe piesa = pozitia unui jucator => +3 puncte\n",
    "        for id, pawn in enumerate(score_tab):\n",
    "                pawn = int(pawn)\n",
    "                if side_game[pawn] == int(first_number) or side_game[pawn] == int(second_number):\n",
    "                    score_tab[id]+=3\n",
    "                    if id + 1 == current_player:\n",
    "                        current_score+=3\n",
    "        \n",
    "        # Scorul din urma mutarii, daca piesa este dublă și punctajul se dublează\n",
    "        # Pentru prima jumatate a piesei\n",
    "        if score1 != 0:\n",
    "            if first_number == second_number:\n",
    "                score1 = score1 * 2\n",
    "            score_tab[current_player-1] += score1\n",
    "            current_score += score1\n",
    "            \n",
    "        # Pentru a doua jumatate a piesei\n",
    "        elif score2 != 0:\n",
    "            if first_number == second_number:\n",
    "                score2 = score2 * 2\n",
    "            score_tab[current_player-1] += score2\n",
